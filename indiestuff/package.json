{
    "name": "indiestuff",
    "nativescript": {
        "id": "org.nativescript.indiestuff"
    },
    "version": "0.0.0",
    "scripts": {
        "tsc": "tsc",
        "tscb": "tsc -b",
        "ng": "ng",
        "prestart": "npm install",
        "start": "ng serve",
        "build": "ng build",
        "test": "ng test",
        "test-headless": "ng test --no-watch --no-progress --browsers=ChromeHeadlessCI",
        "lint": "ng lint indiestuff",
        "lint-fix": "ng lint indiestuff --fix",
        "format": "prettier --write .",
        "e2e": "ng e2e",
        "android": "tns run android --no-hmr",
        "ios": "tns run ios --no-hmr",
        "mobile": "tns run",
        "preview": "tns preview",
        "ngcc": "ngcc --properties es2015 module main --first-only",
        "postinstall": "npm run ngcc",
        "myCustomMemoryComm": "node --max_old_space_size=4096 ./node_modules/@angular/cli/bin/ng"
    },
    "private": true,
    "dependencies": {
        "@angular-eslint/builder": "^12.2.0",
        "@angular-eslint/schematics": "^12.2.0",
        "@angular/animations": "~12.1.1",
        "@angular/cdk": "^12.1.1",
        "@angular/common": "~12.1.1",
        "@angular/compiler": "~12.1.1",
        "@angular/core": "~12.1.1",
        "@angular/forms": "~12.1.1",
        "@angular/material": "^12.1.1",
        "@angular/platform-browser": "~12.1.1",
        "@angular/platform-browser-dynamic": "~12.1.1",
        "@angular/router": "~12.1.1",
        "@nativescript/theme": "~3.0.1",
        "core-js": "~3.15.2",
        "delay": "^5.0.0",
        "howler": "^2.2.3",
        "ngx-color-picker": "^11.0.0",
        "p-retry": "^4.6.0",
        "reflect-metadata": "~0.1.13",
        "rxjs": "~6.5.3",
        "tslib": "^2.3.0",
        "zone.js": "~0.11.4"
    },
    "optionalDependencies": {
        "fsevents": "^2.1.2"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "~12.1.1",
        "@angular/cli": "~12.1.1",
        "@angular/compiler-cli": "~12.1.1",
        "@nativescript/angular": "^12.0.5",
        "@nativescript/core": "^8.0.8",
        "@nativescript/webpack": "~4.1.0",
        "@types/jasmine": "~3.8.1",
        "@types/jasminewd2": "~2.0.10",
        "@types/node": "^16.3.0",
        "@typescript-eslint/eslint-plugin-tslint": "^4.28.2",
        "@typescript-eslint/eslint-plugin": "^4.28.2",
        "@typescript-eslint/parser": "^4.28.2",
        "codelyzer": "^6.0.2",
        "eslint": "^7.30.0",
        "jasmine-core": "~3.8.0",
        "jasmine-spec-reporter": "~7.0.0",
        "karma": "~6.3.4",
        "karma-chrome-launcher": "^3.1.0",
        "karma-coverage-istanbul-reporter": "~3.0.3",
        "karma-jasmine": "~4.0.1",
        "karma-jasmine-html-reporter": "^1.6.0",
        "nativescript": "^8.0.2",
        "protractor": "~7.0.0",
        "ts-node": "~10.0.0",
        "typescript": "^4.3.5",
        "eslint-plugin-import": "latest",
        "eslint-plugin-jsdoc": "latest",
        "eslint-plugin-prefer-arrow": "latest"
    }
}
